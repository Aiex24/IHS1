<Window x:Class="Setup_Manager.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Setup_Manager"
        xmlns:Views="clr-namespace:Setup_Manager.Views"
        xmlns:ViewModels="clr-namespace:Setup_Manager.ViewModels"
        mc:Ignorable="d"
        Title="Setup Manager"

        MinWidth="300"
        ResizeMode="NoResize"
        SizeToContent="WidthAndHeight">
    <Window.DataContext>
        <ViewModels:Initialize/>
    </Window.DataContext>
    <Views:SetupControl/>
</Window>
